@using ToiLamKyThuat.Data.Models
@model List<MasterData>
<nav id="mobile-nav" class="navbar navbar-expand bg-dark" style="z-index:999; padding:8px;">
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav" style="width:100%;">
            @{
                foreach (var item in Model)
                {
                    <li class="nav-item" style="width:100%;">
                        <a class="nav-link text-center" href="@item.Note" style="color:white; padding:0px;">
                            <i class="@item.Icon" style="font-size: 22px;"></i>
                            <p style="font-size: 13px" class="m-0 text-white">@item.CodeName</p>
                        </a>
                    </li>
                }
            }
        </ul>
    </div>
</nav>
